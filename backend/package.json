{
  "name": "game-automation-backend",
  "version": "1.0.0",
  "description": "Game QA Automation Tool - Backend API",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --exec \"ts-node --transpile-only\" src/index.ts",
    "kill:port": "npx kill-port 3001",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "restart": "npx kill-port 3001 && npm run dev",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build:opencv": "cd node_modules/@u4/opencv4nodejs && node-gyp rebuild"
  },
  "keywords": [
    "appium",
    "automation",
    "game",
    "qa",
    "testing"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/node-cron": "^3.0.11",
    "@types/pngjs": "^6.0.5",
    "@types/ws": "^8.18.1",
    "kill-port": "^2.0.1",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.965.0",
